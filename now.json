{
  "version": 2,
  "scope": "mirshko",
  "public": true,
  "env": {
    "DISCOGS_TOKEN": "@discogs_token"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/projects",
      "destination": "/portfolio",
      "statusCode": 301
    },
    {
      "source": "/about",
      "destination": "/",
      "statusCode": 301
    },
    {
      "source": "/contact",
      "destination": "https://twitter.com/mirshko",
      "statusCode": 302
    },
    {
      "source": "/photos",
      "destination": "https://www.instagram.com/mirshko/",
      "statusCode": 302
    },
    {
      "source": "/view-source",
      "destination": "https://github.com/mirshko/dotdesign "
    }
  ]
}
